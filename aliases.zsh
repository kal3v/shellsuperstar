# Iptables
alias ipvl="sudo iptables -vL -n"
alias ipvlt="sudo iptables -t nat -vL -n"
alias ipt="sudo iptstate"

# Netstat
alias natp="netstat -nat"
alias napt="netstat -nat"
alias snatp="sudo netstat -natp"
alias snapt="sudo netstat -natp"
alias snatpl="sudo netstat -natp | grep -i listen"

# Misc
alias psa="ps axf"
alias ll="ls -latr"
alias sl="ls"

# Git
alias gits="git status"

# Vim
alias v="vim"
alias vimrc="vim ~/.vimrc"
